{
  "files.exclude": {
    "**/.vscode": false
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "explorer.excludeGitIgnore": false,
  "files.associations": {
    "*.agent.md": "markdown"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsserver.useSyntaxServer": "never",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
